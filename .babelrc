{
  "presets":[
    "es2015",
    "react",
    "node5",
    "stage-0"
  ],
  "plugins":[
    "array-includes",
    "transform-runtime",
    "transform-object-rest-spread",
    "syntax-class-properties",
    [
      "transform-async-to-module-method",
      {
        "module":"bluebird",
        "method":"coroutine"
      }
    ]
  ]
}